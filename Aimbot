local  mouse = game.Players.LocalPlayer:GetMouse()
local  mt2 = getrawmetatable(game)
local  old2 = mt2.__index
setreadonly(mt2, false)
mt2.__index = newcclosure(function(a, b)
    if   getgenv().aimopen then 
        if a == mouse and b == "Hit" and not checkcaller() then
            return getgenv().silentaimroot
        end
    end
    return old2(a, b)
end)

